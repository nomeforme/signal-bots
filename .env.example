# Signal API URLs
WS_BASE_URL=ws://localhost:8080
HTTP_BASE_URL=http://localhost:8080

# Bot Phone Numbers (comma-separated, must match order in config.json)
BOT_PHONE_NUMBERS=+1234567890,+1234567891,+1234567892

# AI API Keys
GOOGLE_AI_STUDIO_API=your_google_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# AWS Bedrock Credentials (optional, only if using Bedrock models)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_key_here
AWS_REGION=us-east-1

# FAL AI (for image generation, optional)
FAL_KEY=your_fal_key_here
